<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Member">
	<insert id="memberInsert" parameterType="com.team4.bigtower.member.service.Member">
		INSERT INTO member(
			m_id,
			m_pw,
			m_name,
			m_level
		) values (
			#{mId},
			#{mPw},
			#{mName},
			#{mLevel}
		)
	</insert>
</mapper>

